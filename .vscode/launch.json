    "version": "0.2.0",
    "configurations": [
        {
            "name": "(Windows) Launch",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/target/debug/rustdesk.exe",
            //"args": ["--connect", "1664981247"],
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [],
            "console": "externalTerminal"
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug executable 'rustdesk'",
            "cargo": {
                "args": [
                    "build",
                    "--bin=rustdesk",
                    "--package=rustdesk"
                ],
                "filter": {
                    "name": "rustdesk",
                    "kind": "bin"
                }
            },
            "args": [],
            "cwd": "${workspaceFolder}"
        },
   ]
}