{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Android",
            "type": "android-debug", // https://github.com/nisargjhaveri/vscode-android-debug
            "request": "attach",
            "target": "select",
            "pid": "${command:pickAndroidProcess}",
            "mode": "native", // Change to `java` or `native` to attach only Java or Native debugger.
            "packageName": "com.carriez.flutter_hbb", // Package name for your app.
            "native": {
                "symbolSearchPaths": [
                    // "${workspaceFolder}/flutter/android/app/src/main/jniLibs/arm64-v8a/",
                ],
            },
            "java": {
                // "sourcePaths": ["${workspaceFolder}/app/src/main/java"]
            }
        },
        {
            "name": "Remote launch",
            "type": "lldb",
            "request": "custom",
            "initCommands": [
                "platform select remote-android", // Execute `platform list` for a list of available remote platform plugins.
                "platform connect connect://localhost:10086",
                "attach 7124",
                //"cont",
                // "settings set target.inherit-env false", // See note below.
            ],
            //"stopOnEntry": true,
            //"sourceMap": { "/home/" : "/Users/" }
            "preLaunchTask": "build",   
        }
    ]
}